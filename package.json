{
  "name": "healthgram",
  "version": "1.0.2",
  "description": "A web application for travellers to prepare themselves for portential diseases and allergies in their destination.",
  "main": "backend/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node backend/server.js",
    "prestart2": "webpack --mode development",
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "heroku-postbuild": "webpack -d"
  },
  "author": "Red Team",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "mongo-client": "^0.2.1",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.8",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.5",
    "path": "^0.12.7",
    "react-autosuggest": "^9.4.2",
    "react-motion": "^0.5.2",
    "react-scripts": "2.0.5",
    "react-simple-maps": "^0.12.1",
    "recharts": "^1.3.5",
    "request": "^2.88.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.1",
    "express": "^4.16.3",
    "node-sass": "^4.10.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.1"
  },
  "keywords": [
    "health",
